<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FlorianMezzo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FlorianMezzo"
    xmlns:pages="clr-namespace:FlorianMezzo.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="Florian Mezzo">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent Title="HealthCheck">
        <pages:HealthCheck />
    </ShellContent>

</Shell>
